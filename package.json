{
  "name": "@petsel/es-type-detection",
  "version": "0.1.0",
  "description": "A utility library for consistent and extensible type detection in JavaScript.",
  "license": "MIT",
  "funding": {
    "type": "custom",
    "url": "https://github.com/petsel/es-type-detection"
  },
  "main": "dist/es-type-detection.cjs.js",
  "module": "dist/es-type-detection.es.js",
  "unpkg": "dist/es-type-detection.umd.js",
  "jsdelivr": "dist/es-type-detection.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "release:mock": "npm run lint && npm run format && npm run test && npm run coverage && npm run docs && npm run build",
    "rollup:build": "rollup -c",
    "build": "vite build",
    "lint": "eslint src/**/*.js test/**/*.js",
    "lint:check": "npm run lint -- --quiet",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "docs:docdash": "jsdoc -c jsdoc.docdash.json",
    "docs": "npm run docs:docdash",
    "dev:docs": "vitepress dev docs",
    "build:docs": "vitepress build docs",
    "preview:docs": "npm run build:docs && vitepress preview docs"
  },
  "dependencies": {
    "vite": "^6.2.3",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-v8": "^3.0.9",
    "docdash": "^2.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-yml": "^1.17.0",
    "husky": "^9.1.7",
    "jsdoc": "^4.0.4",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rollup": "^4.37.0",
    "vite-plugin-eslint": "^1.8.1",
    "yaml-eslint-parser": "^1.3.0"
  }
}
